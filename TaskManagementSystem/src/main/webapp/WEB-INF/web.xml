<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="https://jakarta.ee/xml/ns/jakartaee"
	xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_5_0.xsd"
	id="WebApp_ID" version="5.0">
	<display-name>TaskManagementSystem</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.jsp</welcome-file>
		<welcome-file>default.htm</welcome-file>
	</welcome-file-list>
	<servlet>
		<description></description>
		<display-name>Register</display-name>
		<servlet-name>Register</servlet-name>
		<servlet-class>com.taskmanager.Register</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Register</servlet-name>
		<url-pattern>/register</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>Login</display-name>
		<servlet-name>Login</servlet-name>
		<servlet-class>com.taskmanager.Login</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>Dashboard</display-name>
		<servlet-name>Dashboard</servlet-name>
		<servlet-class>com.taskmanager.Dashboard</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Dashboard</servlet-name>
		<url-pattern>/dashboard</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>EditTask</display-name>
		<servlet-name>EditTask</servlet-name>
		<servlet-class>com.taskmanager.EditTask</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>EditTask</servlet-name>
		<url-pattern>/editTask</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>GetTaskById</display-name>
		<servlet-name>GetTaskById</servlet-name>
		<servlet-class>com.taskmanager.GetTaskById</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GetTaskById</servlet-name>
		<url-pattern>/getTaskById</url-pattern>
	</servlet-mapping>
</web-app>